      "required_elements": [
        "history of interactions with Fred",
        "notable conflicts or agreements",
        "Fred's perception of character",
        "character's strategies for managing Fred",
        "impact of Fred's decisions on character"
      ],
      "priority_by_character_type": {
        "primary": "CRITICAL",
        "secondary": "HIGH",
        "minor": "MEDIUM"
      },
      "task_category": "CHARACTER",
      "task_template": "fred_interaction_development"
    },
    "corporate_feuds": {
      "pattern": "feud|conflict|argument|disagreement|battle|rivalry|dispute",
      "required_files": [
        "{character}/career/feuds/character-{character}-corporate-feuds.md"
      ],
      "required_elements": [
        "major corporate rivalries",
        "interdepartmental conflicts",
        "professional disagreements",
        "conflict resolution approaches",
        "long-term impact of feuds"
      ],
      "priority_by_character_type": {
        "primary": "HIGH",
        "secondary": "HIGH",
        "minor": "MEDIUM"
      },
      "task_category": "CHARACTER",
      "task_template": "corporate_feuds_development"
    },
    "psychological_development": {
      "pattern": "psychology|mental|personality formation|psychological",
      "required_files": [
        "{character}/origins/psychology/character-{character}-psychological-development.md"
      ],
      "required_elements": [
        "core personality traits origin",
        "psychological evolution",
        "defining personality events",
        "characteristic behavioral patterns"
      ],
      "priority_by_character_type": {
        "primary": "HIGH",
        "secondary": "MEDIUM",
        "minor": "LOW"
      },
      "task_category": "CHARACTER",
      "task_template": "psychological_development"
    },
    "family_connections": {
      "pattern": "family|relatives|parents|siblings|children|spouse",
      "required_files": [
        "{character}/family/character-{character}-family-connections.md"
      ],
      "required_elements": [
        "immediate family",
        "significant family relationships",
        "family influence on character",
        "family dynamics"
      ],
      "priority_by_character_type": {
        "primary": "MEDIUM",
        "secondary": "LOW",
        "minor": "LOW"
      },
      "task_category": "CHARACTER",
      "task_template": "family_development"
    }
