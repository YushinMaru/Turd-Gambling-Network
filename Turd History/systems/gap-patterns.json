{
  "version": "1.0.0",
  "created_by": "AUTO-010",
  "last_modified": "2025-05-07",
  
  "character_gaps": {
    "childhood": {
      "pattern": "childhood|early life|formative years|youth|young",
      "required_files": [
        "{character}/origins/character-{character}-childhood-brief.md"
      ],
      "required_elements": [
        "brief family background",
        "essential formative experiences",
        "key childhood traits"
      ],
      "priority_by_character_type": {
        "primary": "LOW",
        "secondary": "LOW",
        "minor": "LOW"
      },
      "task_category": "CHARACTER",
      "task_template": "childhood_development_brief"
    },
    "education": {
      "pattern": "education|school|university|college|degree|academic",
      "required_files": [
        "{character}/origins/character-{character}-education.md"
      ],
      "required_elements": [
        "institutions attended",
        "fields of study",
        "academic achievements",
        "influential mentors"
      ],
      "priority_by_character_type": {
        "primary": "MEDIUM",
        "secondary": "MEDIUM", 
        "minor": "LOW"
      },
      "task_category": "CHARACTER",
      "task_template": "education_development"
    },
    "career_progression": {
      "pattern": "career|professional|job|position|employment|work history",
      "required_files": [
        "{character}/career/phases/character-{character}-career-early.md",
        "{character}/career/phases/character-{character}-career-turdbird.md"
      ],
      "required_elements": [
        "first employment",
        "career transitions",
        "major achievements",
        "professional skills development",
        "role at Turd Bird Industries"
      ],
      "priority_by_character_type": {
        "primary": "HIGH",
        "secondary": "HIGH",
        "minor": "MEDIUM"
      },
      "task_category": "CHARACTER",
      "task_template": "career_development"
    },
    "work_dynamics": {
      "pattern": "work dynamics|office politics|workplace|professional relationships|colleagues",
      "required_files": [
        "{character}/career/character-{character}-work-dynamics.md"
      ],
      "required_elements": [
        "office alliances and rivalries",
        "political positioning",
        "reputation among colleagues",
        "management style",
        "daily work interactions"
      ],
      "priority_by_character_type": {
        "primary": "HIGH",
        "secondary": "HIGH",
        "minor": "MEDIUM"
      },
      "task_category": "CHARACTER",
      "task_template": "work_dynamics_development"
    },
    "interactions_with_fred": {
      "pattern": "fred|founder|ceo|boss|turd",
      "required_files": [
        "{character}/relationships/relationship-{character}-fred-turd-professional.md"
