  "relationship_gaps": {
    "professional_relationship": {
      "pattern": "professional relationship|work relationship|colleagues|office relationship",
      "required_files": [
        "relationships/professional/relationship-{character1}-{character2}-professional.md"
      ],
      "required_elements": [
        "work collaboration patterns",
        "professional respect or tension",
        "project interactions",
        "workplace communication style",
        "management/reporting dynamics"
      ],
      "priority_calculation": "max(character1_priority, character2_priority)",
      "task_category": "RELATIONSHIP",
      "task_template": "professional_relationship_development"
    },
    "first_meeting": {
      "pattern": "first meeting|met|initial encounter|introduction|first encounter",
      "required_files": [
        "relationships/{type}/relationship-{character1}-{character2}-first-meeting.md"
      ],
      "required_elements": [
        "circumstances of meeting",
        "initial impressions",
        "context of introduction",
        "immediate aftermath"
      ],
      "priority_calculation": "max(character1_priority, character2_priority)",
      "task_category": "RELATIONSHIP",
      "task_template": "first_meeting_development"
    },
    "relationship_evolution": {
      "pattern": "evolution|development|progression|change|dynamic",
      "required_files": [
        "relationships/{type}/relationship-{character1}-{character2}-evolution.md"
      ],
      "required_elements": [
        "key relationship phases",
        "significant events affecting relationship",
        "turning points",
        "pattern development"
      ],
      "priority_calculation": "max(character1_priority, character2_priority)",
      "task_category": "RELATIONSHIP",
      "task_template": "relationship_evolution"
    },
    "conflict_origin": {
      "pattern": "conflict|animosity|rivalry|antagonism|dispute|tension",
      "required_files": [
        "relationships/{type}/relationship-{character1}-{character2}-conflict-origin.md"
      ],
      "required_elements": [
        "source of conflict",
        "initial conflict event",
        "escalation pattern",
        "ongoing conflict manifestations"
      ],
      "priority_calculation": "max(character1_priority, character2_priority)",
      "task_category": "RELATIONSHIP",
      "task_template": "conflict_origin_development"
    },
    "power_dynamics": {
      "pattern": "power|authority|influence|control|dominance|submission",
      "required_files": [
        "relationships/{type}/relationship-{character1}-{character2}-power-dynamics.md"
      ],
      "required_elements": [
        "power balance",
        "authority patterns",
        "influence methods",
        "evolution of control dynamics"
      ],
      "priority_calculation": "character1_primary && character2_primary ? HIGH : MEDIUM",
      "task_category": "RELATIONSHIP",
      "task_template": "power_dynamics_development"
    }
  },
  
  "timeline_gaps": {
    "unexplained_periods": {
      "pattern": "gap|missing|unexplained|unclear period|unknown time",
      "detection_method": "date_sequence_analysis",
      "minimum_gap_months": 12,
      "required_files": [
        "timeline/character/{character}/timeline-{character}-{year1}-{year2}.md"
      ],
      "required_elements": [
        "activities during period",
        "significant events",
        "character development",
        "connections to established timeline"
      ],
      "priority_by_character_type": {
        "primary": "HIGH",
        "secondary": "MEDIUM",
        "minor": "LOW"
      },
      "task_category": "TIMELINE",
      "task_template": "unexplained_period_development"
    },
    "key_event_impacts": {
      "pattern": "impact|effect|influence|result|consequence|aftermath",
      "detection_method": "event_impact_analysis",
      "required_files": [
        "timeline/events/timeline-{year}-{event}-impact.md"
      ],
      "required_elements": [
        "immediate consequences",
        "long-term effects",
        "character reactions",
        "organizational changes"
      ],
      "priority_by_event_significance": {
        "major": "HIGH",
        "moderate": "MEDIUM",
        "minor": "LOW"
      },
      "task_category": "TIMELINE",
      "task_template": "event_impact_development"
    }
  },
  
